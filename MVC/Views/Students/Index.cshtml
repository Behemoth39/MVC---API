@model IList<WestCoastEducation.web.ViewModels.PersonViewModel>

@{
    ViewBag.Pagetitle = "Students";
}

<section class="main-content">
    <div class="main-content-row-1">   
        <img src="~/Media/school.png" alt="School" height="800" width="800" />
    </div>
    <div class="main-content-row-2">
        <h1>Studenter</h1>
        <p>Detta ser inte snyggt ut.......</p>   
        <ul>
            @foreach (var student in Model)
            {
                <p><a asp-action="details" asp-route-studentId="@student.Id">
                        @student.FirstName - @student.LastName
                </a></p>
            }
        </ul>
    </div>
</section>
